# this will display the results as human years for a dogs age


def display_results(age, dog_age):
  print (("your dogs age in human years is") + str(dog_age))


def get_age():
  print("What is the age of your dog in human years?")
  age = int(input())
  return age


def process_age(age):
  dog_age = age * 7
  return dog_age


# Main
def main():
  if Age > 0:
    process_age(age, dog_age)
  else:
    print("Please Enter a whole number for your dogs age")
age = get_age   
dog_age = process_age(age)
Age = process_age(age, dog_age)


main()
